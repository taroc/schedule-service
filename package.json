{
  "name": "schedule-service",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@1.22.19",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate --no-engine && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "db:studio": "prisma studio",
    "seed": "tsx scripts/seed-test-data.ts",
    "migration:deploy": "npx prisma migrate deploy",
    "migration:reset": "npx prisma migrate reset",
    "migration-prod:create": "dotenv -e .env.production npx prisma migrate dev --name",
    "migration-prod:deploy": "dotenv -e .env.production npx prisma migrate deploy",
    "seed:prod": "NODE_ENV=production tsx scripts/seed-production.ts",
    "deploy:prod": "npm run migrate:prod && npm run seed:prod"
  },
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "@prisma/extension-accelerate": "^2.0.1",
    "bcryptjs": "^3.0.2",
    "jsonwebtoken": "^9.0.2",
    "next": "15.3.3",
    "node-fetch": "^3.3.2",
    "prisma": "^6.9.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "vercel": "^43.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dotenv": "^8.2.3",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/ui": "^3.2.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "jsdom": "^26.1.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.3",
    "typescript": "^5",
    "vite": "^6.3.5",
    "vitest": "^3.2.3"
  }
}
